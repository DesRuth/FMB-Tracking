<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pkvv.fmb_tracking">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.DriverHomeActivity"></activity>
        <activity android:name=".ui.PassangerHomeActivity" />
        <activity android:name=".ui.DriverRegisterActivity" />
        <activity android:name=".ui.DriverLoginActivity" />
        <activity android:name=".ui.PassangerRegisterActivity" />
        <activity android:name=".ui.PassangerLoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>